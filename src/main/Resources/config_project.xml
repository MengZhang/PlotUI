    </Configuration>
    <Configuration name="ClimAnomaly">
        <plotVar>TMAX</plotVar>
        <plotType>relative</plotType>
        <plotFormat>PNG</plotFormat>
        <inputDir></inputDir>
        <outputPath>plot_output</outputPath>
        <outputGraph>ClimAnomaly</outputGraph>
    </Configuration>
