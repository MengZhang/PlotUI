<Configurations>
    <Configuration name="GlobalConfig">
        <RLibPath>default</RLibPath>
        <RExePath>default</RExePath>
        <GcmMapping>0XFX:Base:#D3D3D3|</GcmMapping>
    </Configuration>
    <Configuration name="StandardPlot">
        <title>Standard Plot</title>
        <plotType>BoxPlot</plotType>
        <plotFormat>PNG</plotFormat>
        <plotVar>HWAH_S</plotVar>
        <plotMethod>ABSOLUTE</plotMethod>
        <inputDir>CM1</inputDir>
        <inputDir2>CM2</inputDir2>
        <outputPath>plot_output</outputPath>
        <outputACMO>true</outputACMO>
        <outputGraph>StdPlot</outputGraph>
